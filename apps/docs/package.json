{
  "name": "docs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-s typecheck build:react-router build:netlify-prepare",
    "build:react-router": "react-router build",
    "build:netlify-prepare": "node netlify/prepare.js",
    "dev": "react-router dev",
    "start": "netlify serve",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "lint": "run-s eslint stylelint",
    "eslint": "eslint '*/**/*.{js,ts,jsx,tsx}' --fix",
    "stylelint": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@netlify/functions": "2.8.2",
    "@react-router/node": "^7.0.1",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1"
  },
  "devDependencies": {
    "@mjackson/node-fetch-server": "0.3.0",
    "@react-router/dev": "^7.0.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@svgr/rollup": "^8.1.0",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "sass": "^1.81.0",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.36.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.2"
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
